<Project Sdk="Peachpie.NET.Sdk/1.1.9">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net7.0</TargetFramework>

    <Description>.NET class library in PHP</Description>
    <AssemblyName>Flarum</AssemblyName>
    <ComposerJsonPath>
    </ComposerJsonPath>
    <ComposerAutoload>true</ComposerAutoload>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="
             flarum;
             **/*.php;
             "
             Exclude="
             obj/**;
             bin/**;
             vendor74/**;
             vendor82/**;
             vendor/symfony/polyfill-iconv/**;
             vendor/illuminate/database/Eloquent/BroadcastableModelEventOccurred.php;
             vendor/illuminate/queue/CallQueuedClosure.php;
             vendor/illuminate/filesystem/Cache.php;
             vendor/symfony/console/Tester/Constraint/CommandIsSuccessful.php;         
             vendor/symfony/translation/DependencyInjection/*.php;
             vendor/symfony/console/DependencyInjection/AddConsoleCommandPass.php;
             vendor/symfony/event-dispatcher/DependencyInjection/*.php;
             vendor/symfony/translation/DataCollector/TranslationDataCollector.php;
             vendor/symfony/translation/DataCollectorTranslator.php;
             vendor/symfony/mime/DependencyInjection/AddMimeTypeGuesserPass.php;
             vendor/symfony/finder/Iterator/RecursiveDirectoryIterator.php;
             vendor/symfony/http-foundation/Session/Storage/Handler/IdentityMarshaller.php;

             vendor/doctrine/cache/lib/Doctrine/Common/Cache/Psr6/CacheAdapter.php;
             vendor/doctrine/cache/lib/Doctrine/Common/Cache/Psr6/InvalidArgument.php;
             vendor/doctrine/cache/lib/Doctrine/Common/Cache/Psr6/TypedCacheItem.php;
             vendor/doctrine/cache/lib/Doctrine/Common/Cache/Psr6/TypedCacheItem.php;
             vendor/doctrine/cache/lib/Doctrine/Common/Cache/Psr6/CacheItem.php;
             vendor/opis/closure/src/Analyzer.php;
             vendor/intervention/image/src/Intervention/Image/ImageServiceProviderLeague.php;
            
             vendor/nesbot/carbon/src/Carbon/PHPStan/MacroExtension.php;
             vendor/nesbot/carbon/src/Carbon/PHPStan/Macro.php;
             vendor/nesbot/carbon/src/Carbon/PHPStan/AbstractMacro.php;
             vendor/nesbot/carbon/src/Carbon/MessageFormatter/**/*.php;
             vendor/nesbot/carbon/lazy/Carbon/PHPStan/*.php;
             vendor/nesbot/carbon/lazy/Carbon/MessageFormatter/**/*.php;
             vendor/illuminate/database/PDO/*.php;
             **/Testing/**;
             **/Tests/**;
             **/Test/**;
             " />
    <Content Include="**" Exclude="
      obj/**;
      bin/**;
      .vs/**;
      .vscode/**;
      *.manifest;readme.html;
      public/**/*.php;
      storage/**/*.php;
      *.php;
      *.md;
      *.msbuildproj;*.msbuildproj.user;
      " CopyToOutputDirectory="PreserveNewest">
     
    </Content>
  </ItemGroup>
  <!--<ItemGroup>
    <PackageReference Include="Peachpie.Library.Graphics" Version="1.1.7" />
    <PackageReference Include="Peachpie.Runtime" Version="1.1.7" />
    
  </ItemGroup>-->
  <ItemGroup>
    <PackageReference Update="Peachpie.App" Version="1.1.9" />
  </ItemGroup>
</Project>